{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python@4.0.0"
    },
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/app.py"
    },
    {
      "src": "/admin",
      "dest": "/admin.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}

